    // Delete output files
    ListFolderFiles(0,'%O','.mps',0,1);
    LstEnum('%!0','%\');
    LstGetCount(1);
    Repeat(%!1,>
      !`LstGetItem(2,%!c-1);>
        GetFileName('%!2',3);>
        FindInString('%!3','<\"|%N|-+$Numeric$~$~[]$',5,6,1);>
        IfOK(!|DeleteFile("%O\%!2");|);>
        ProcessMessages;>
       `);
    LstRelease;
    ListFolderFiles(0,'%O','',0,1);
